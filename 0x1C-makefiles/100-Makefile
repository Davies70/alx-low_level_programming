CC = gcc
SRC = main.c school.c
OBJ = $(SRC:.c=.o)
NAME = school
DEL = rm -f
CFLAGS = -Wall -Werror -Wextra -pedantic
FLAGS = $(CFLAG)

all: $(OBJ) m.h
	$(CC) $(FLAGS) $(OBJ) -o $(NAME)

.PHONY: fclean clean oclean
.PHONY: re oclean
.PHONY: clean
.PHONY: oclean

clean:
	$(REM) *~ $(NAME)


oclean:
	$(REM) $(OBJ)

fclean: clean oclean

re: oclean all





